 column=(type=char(297) updatewhereclause=yes name=kyohi_bumon_full dbname="kyohi_bumon_full" )
 column=(type=decimal(0) updatewhereclause=yes name=kyohi_kbn dbname="kyohi_kbn" )
 column=(type=char(7) updatewhereclause=yes name=kyohi_kbn_full dbname="kyohi_kbn_full" )
 column=(type=char(19) updatewhereclause=yes name=touroku_date dbname="touroku_date" )
 column=(type=char(20) updatewhereclause=yes name=m_user_touroku_user_mei dbname="touroku_user_mei" )
 column=(type=char(19) updatewhereclause=yes name=henkou_date dbname="henkou_date" )
 column=(type=char(20) updatewhereclause=yes name=m_user_henkou_user_mei dbname="henkou_user_mei" )
 retrieve="
select rownum,
       mtk.kyohi_syubetu,
       mei_163.meisyo_1	kyohi_syubetu_mei,
       to_char(mtk.kyohi_syubetu,'000') || ':' ||  mei_163.meisyo_1 as kyohi_syubetu_full,
       mtk.kyohi_bumon,
       mei_167.meisyo_1 kyohi_bumon_mei,
       mtk.kyohi_bumon || ':' ||  mei_167.meisyo_1 as kyohi_bumon_full,
       mtk.kyohi_kbn,
       decode( mtk.kyohi_kbn, 1, '1:通知不要', 2, '2:メール不要' )	kyohi_kbn_full,
       to_char( mtk.touroku_date, 'yyyy/mm/dd hh24:mi:ss' ) touroku_date,
       mu1.user_mei        touroku_user_mei,
       to_char( mtk.henkou_date, 'yyyy/mm/dd hh24:mi:ss' )  henkou_date,
       mu2.user_mei        henkou_user_mei
from   m_tuuti_filter mtk,
       m_user       mu1,
       m_user       mu2,
       ( select meisyo_code, meisyo_1 from m_meisyo where meisyo_kbn = 163 )    mei_163,
       ( select meisyo_code, meisyo_1 from m_meisyo where meisyo_kbn = 167 )    mei_167
 where mtk.touroku_user_id  = mu1.user_id(+)
   and mtk.henkou_user_id   = mu2.user_id(+)
   and mtk.kyohi_syubetu	= mei_163.meisyo_code(+)
   and mtk.kyohi_bumon		= mei_167.meisyo_code(+)
   and mtk.user_id          = :p_user_id
 order by mtk.kyohi_syubetu, mtk.kyohi_bumon
" arguments=(("p_user_id", string)) )
