table(column=(type=number updatewhereclause=yes name=rownum dbname="rownum" )
 column=(type=decimal(0) updatewhereclause=yes name=kyohi_syubetu dbname="kyohi_syubetu" )
 column=(type=char(256) updatewhereclause=yes name=kyohi_syubetu_mei dbname="kyohi_syubetu_mei" )
 column=(type=char(261) updatewhereclause=yes name=kyohi_syubetu_full dbname="kyohi_syubetu_full" )
 column=(type=decimal(0) updatewhereclause=yes name=kyohi_bumon dbname="kyohi_bumon" )
