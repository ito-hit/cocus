 retrieve="select row_number() over ( order by syohin.syohin_code, syohin.iro_no ) ~"rownum~",
       max( to_number( to_char( syohin.syohin_code ) || lpad( to_char( syohin.iro_no ), 3, '0' ) ) ) kensaku_code,
       syohin.syohin_code, syohin.iro_no,
       max( trim( syohin.hin_ban ) ) hin_ban,
       max( trim( syohin.hin_mei ) ) hin_mei,
       max( trim( syohin.iro_mei ) ) iro_mei,
       max( trim( syohin.hin_mei_kana ) ) hin_mei_kana,
       max( trim( syohin.iro_mei_kana ) ) iro_mei_kana,
       max( decode( syohin.bumon_code, 0, '', to_char( syohin.bumon_code ) || ':' || mei_bumon.meisyo ) ) bumon,
       max( decode( syohin.kyoyo_flg, 1, '1:対象', '' ) ) kyoyo_flg,
       max( syohin.hatubai_date ) hatubai_date,
       max( decode( syohin.haiban_kbn, 0, '', to_char( syohin.haiban_kbn ) || ':' || mei_haiban.meisyo ) ) haiban_kbn,
       max( syohin.haiban_date ) haiban_date,
       max( decode( syohin.haiban_riyu_kbn, 0, '', to_char( syohin.haiban_riyu_kbn ) || ':' || mei_hairiyu.meisyo ) ) haiban_riyu_kbn,
       max( decode( syohin.syusoku_flg, 1, '1:対象', '' ) ) syusoku_flg,
       max( syohin.jyoudai ) jyoudai,
       max( decode( syohin.jyoudai_koukai_flg, 1, '1:対象', '' ) ) jyoudai_koukai_flg,
       max( syohin.kyuu_jyoudai ) kyuu_jyoudai,
       max( syohin.jyoudai_henkou_date ) jyoudai_henkou_date,
       max( syohin.sin_jyoudai ) sin_jyoudai,
       max( syohin.sin_jyoudai_tekiyou_date ) sin_jyoudai_tekiyou_date,
       max( decode( syohin.kakaku_group_kbn, 0, '', to_char( syohin.kakaku_group_kbn ) || ':' || mei_kakaku_grp.meisyo ) ) kakaku_group_kbn,
       max( decode( syohin.tanka_touroku_flg, 1, '1:対象', '' ) ) tanka_touroku_flg,
       max( decode( syohin.tankahyo_hakkou_kbn, 0, '', to_char( syohin.tankahyo_hakkou_kbn ) || ':' || mei_tankahyo.meisyo ) ) tankahyo_hakkou_kbn,
       max( syohin.jissai_genka ) jissai_genka,
       max( syohin.kibou_kouri_kakaku ) kibou_kouri_kakaku,
       max( decode( syohin.seihin_syohin_kbn, 0, '', to_char( syohin.seihin_syohin_kbn ) || ':' || mei_seisyo.meisyo ) ) seihin_syohin_kbn,
       max( decode( trim( seisankoku_code ), '0', '', null, '', to_char( seisankoku_code ) || ':' || mei_seisankoku.meisyo ) ) seisankoku_code,
       max( decode( trim( syohin.siiresaki_code ), '0', '', null, '', to_char( syohin.siiresaki_code ) || ':' || trim( kojyo.kojyo_mei ) ) ) siiresaki_code_x,
       max( decode( syohin.siirehin_flg, 1, '1:対象', '' ) ) siirehin_flg,
       max( decode( syohin.syohin_kukaku_kbn, 0, '', to_char( syohin_kukaku_kbn ) || ':' || mei_kikaku.meisyo ) ) syohin_kukaku_kbn,
       max( decode( syohin.bettyu_running_flg, 1, '1:対象', '' ) ) bettyu_running_flg,
       max( decode( trim( syohin.tokui_code ), '0', '', null, '', to_char( syohin.tokui_code ) || ':' || trim( tokui.tokui_mei ) ) ) tokui_code_x,
       max( decode( trim( syohin.bukken_code ), '0', '', null, '', to_char( syohin.bukken_code ) || ':' || trim( bukken.bukken_mei ) ) ) bukken_code_x,
       max( decode( syohin.tokutei_tokui_kbn, 0, '', to_char( syohin.tokutei_tokui_kbn ) || ':' || mei_tokutei.meisyo ) ) tokutei_tokui_kbn,
       max( syohin.irisu ) irisu,
       max( syohin.ball_irisu ) ball_irisu,
       max( decode( syohin.tani_kbn, 0, '', to_char( syohin.tani_kbn ) || ':' || mei_tani.meisyo ) ) tani_kbn,
       max( decode( syohin.syozai_kbn, 0, '', to_char( syohin.syozai_kbn ) || ':' || mei_syozai.meisyo ) ) syozai_kbn,
       max( decode( syohin.zaiko_kanri_flg, 1, '1:対象', '' ) ) zaiko_kanri_flg,
       max( decode( syohin.suryo_keisan_flg, 1, '1:対象', '' ) ) suryo_keisan_flg,
       max( decode( syohin.kingaku_keisan_flg, 1, '1:対象', '' ) ) kingaku_keisan_flg,
       max( decode( syohin.arari_keisan_flg, 1, '1:対象', '' ) ) arari_keisan_flg,
       max( decode( syohin.kankyo_hairyo_flg, 1, '1:対象', '' ) ) kankyo_hairyo_flg,
       max( decode( syohin.kuuyu_kinsi_flg, 1, '1:対象', '' ) ) kuuyu_kinsi_flg,
       max( decode( syohin.syokuti_flg, 1, '1:対象', '' ) ) syokuti_flg,
       max( decode( syohin.bhin_flg, 1, '1:対象', '' ) ) bhin_flg,
       max( decode( trim( syohin.daibunrui_code ), '0', '', null, '', to_char( syohin.daibunrui_code ) || ':' || trim( bunrui.daibunrui_mei ) ) ) daibunrui_code_x,
       max( decode( trim( syohin.tyubunrui_code ), '0', '', null, '', to_char( syohin.tyubunrui_code ) || ':' || trim( bunrui.tyubunrui_mei ) ) ) tyubunrui_code_x,
       max( decode( trim( syohin.syobunrui_code ), '0', '', null, '', to_char( syohin.syobunrui_code ) || ':' || trim( bunrui.syobunrui_mei ) ) ) syobunrui_code_x,
       max( decode( trim( syohin.group_code ), '0', '', null, '', to_char( syohin.group_code ) || ':' || trim( groupm.group_mei ) ) ) group_code_x,
       max( decode( trim( syohin.brand_code ), '0', '', null, '', to_char( syohin.brand_code ) || ':' || trim( brand.brand_mei ) ) ) brand_code_x,
       max( decode( trim( syohin.subbrand_code ), '0', '', null, '', to_char( syohin.subbrand_code ) || ':' || trim( subbrand.brand_mei ) ) ) subbrand_code_x,
       max( decode( trim( syohin.season_code ), '0', '', null, '', to_char( syohin.season_code ) || ':' || trim( mei_season.meisyo ) ) ) season_code_x,
       max( syohin.seriese_code ) seriese_code,
       max( syohin.sozai ) sozai,
       max( syohin.konritu_1 ) konritu_1,
       max( syohin.konritu_2 ) konritu_2,
       max( syohin.konritu_3 ) konritu_3,
       max( syohin.konritu_4 ) konritu_4,
       max( syohin.konritu_5 ) konritu_5,
       max( syohin.konritu_6 ) konritu_6,
       max( syohin.konritu_7 ) konritu_7,
       max( syohin.konritu_8 ) konritu_8,
       max( syohin.konritu_9 ) konritu_9,
       max( syohin.konritu_10 ) konritu_10,
       max( syohin.yoki_kami ) yoki_kami,
       max( syohin.yoki_pla ) yoki_pla,
       max( syohin.housou_kami ) housou_kami,
       max( syohin.housou_pla ) housou_pla,
       max( syohin.touroku_date ) touroku_date,
       max( syohin.henkou_date ) henkou_date
  from m_syohin syohin,
       ( select to_number( meisyo_code ) meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
          where meisyo_kbn	= 160
       ) mei_bumon,
       ( select to_number( meisyo_code ) meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
           where meisyo_kbn	= 122
       ) mei_haiban,
       ( select to_number( meisyo_code ) meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
           where meisyo_kbn	= 123
       ) mei_hairiyu,
       ( select to_number( meisyo_code ) meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
           where meisyo_kbn	= 131
       ) mei_kakaku_grp,
       ( select to_number( meisyo_code ) meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
           where meisyo_kbn	= 159
       ) mei_tankahyo,
       ( select to_number( meisyo_code ) meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
           where meisyo_kbn	= 126
       ) mei_seisyo,
       ( select meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
           where meisyo_kbn	= 130
       ) mei_seisankoku,
       ( select to_number( meisyo_code ) meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
           where meisyo_kbn	= 120
       ) mei_kikaku,
       ( select to_number( meisyo_code ) meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
           where meisyo_kbn	= 129
       ) mei_tokutei,
       ( select to_number( meisyo_code ) meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
           where meisyo_kbn	= 125
       ) mei_tani,
       ( select to_number( meisyo_code ) meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
           where meisyo_kbn	= 127
       ) mei_syozai,
       ( select to_number( meisyo_code ) meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
           where meisyo_kbn	= 121
       ) mei_season,
       ( select kojyo_code, kojyo_mei
           from sy_m_kojyo
          where bumon_code	= 9
       ) kojyo,
       m_tokui tokui,
       m_bukken bukken,
       m_bunrui bunrui,
       m_group groupm,
       m_brand brand,
       m_brand subbrand,
       m_gtin gtin,
       m_user musert,
       m_user muserh
 where syohin.bumon_code			= mei_bumon.meisyo_code(+)
   and syohin.haiban_kbn			= mei_haiban.meisyo_code(+)
   and syohin.haiban_riyu_kbn		= mei_hairiyu.meisyo_code(+)
   and syohin.kakaku_group_kbn		= mei_kakaku_grp.meisyo_code(+)
   and syohin.tankahyo_hakkou_kbn	= mei_tankahyo.meisyo_code(+)
   and syohin.seihin_syohin_kbn		= mei_seisyo.meisyo_code(+)
   and syohin.seisankoku_code		= mei_seisankoku.meisyo_code(+)
   and syohin.syohin_kukaku_kbn		= mei_kikaku.meisyo_code(+)
   and syohin.tokutei_tokui_kbn		= mei_tokutei.meisyo_code(+)
   and syohin.tani_kbn				= mei_tani.meisyo_code(+)
   and syohin.syozai_kbn			= mei_syozai.meisyo_code(+)
   and syohin.season_code			= mei_season.meisyo_code(+)
   and syohin.siiresaki_code		= kojyo.kojyo_code(+)
   and syohin.tokui_code			= tokui.tokui_code(+)
   and syohin.bukken_code			= bukken.bukken_code(+)
   and syohin.daibunrui_code		= bunrui.daibunrui_code(+)
   and syohin.tyubunrui_code		= bunrui.tyubunrui_code(+)
   and syohin.syobunrui_code		= bunrui.syobunrui_code(+)
   and syohin.group_code			= groupm.group_code(+)
   and syohin.brand_code			= brand.brand_code(+)
   and syohin.subbrand_code			= subbrand.brand_code(+)
   and syohin.sku_code				= gtin.sku_code(+)
   and gtin.code_kbn(+)				= 1
   and syohin.touroku_user_id		= musert.user_id(+)
   and syohin.henkou_user_id		= muserh.user_id(+)
 group by syohin.syohin_code, syohin.iro_no
