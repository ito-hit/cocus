 retrieve="select row_number() over ( order by syohin.syohin_code, syohin.iro_no, syohin.size_iti ) ~"rownum~",
       to_number( to_char( syohin.syohin_code ) || lpad( to_char( syohin.iro_no ), 3, '0' ) ) kensaku_code,
       syohin.syohin_code, syohin.iro_no,
       trim( syohin.hin_ban ) hin_ban,
       trim( syohin.hin_mei ) hin_mei,
       trim( syohin.iro_mei ) iro_mei,
       trim( syohin.hin_mei_kana ) hin_mei_kana,
       trim( syohin.iro_mei_kana ) iro_mei_kana,
       decode( syohin.bumon_code, 0, '', to_char( syohin.bumon_code ) || ':' || mei_bumon.meisyo ) bumon,
       decode( syohin.kyoyo_flg, 1, '1:対象', '' ) kyoyo_flg,
       syohin.hatubai_date,
       decode( syohin.haiban_kbn, 0, '', to_char( syohin.haiban_kbn ) || ':' || mei_haiban.meisyo ) haiban_kbn,
       syohin.haiban_date,
       decode( syohin.haiban_riyu_kbn, 0, '', to_char( syohin.haiban_riyu_kbn ) || ':' || mei_hairiyu.meisyo ) haiban_riyu_kbn,
       decode( syohin.syusoku_flg, 1, '1:対象', '' ) syusoku_flg,
       syohin.jyoudai,
       decode( syohin.jyoudai_koukai_flg, 1, '1:対象', '' ) jyoudai_koukai_flg,
       syohin.kyuu_jyoudai,
       syohin.jyoudai_henkou_date,
       syohin.sin_jyoudai,
       syohin.sin_jyoudai_tekiyou_date,
       decode( syohin.kakaku_group_kbn, 0, '', to_char( syohin.kakaku_group_kbn ) || ':' || mei_kakaku_grp.meisyo ) kakaku_group_kbn,
       syohin.size_iti, syohin.size_code,
       trim( syohin.size_mei ) size_mei,
       decode( syohin.hi_toriatukai_flg, 1, '1:対象', '' ) hi_toriatukai_flg,
       gtin.gtin_code jan_code,
       syohin.up_gaku, syohin.up_iti,
       decode( syohin.tanka_touroku_flg, 1, '1:対象', '' ) tanka_touroku_flg,
       decode( syohin.tankahyo_hakkou_kbn, 0, '', to_char( syohin.tankahyo_hakkou_kbn ) || ':' || mei_tankahyo.meisyo ) tankahyo_hakkou_kbn,
       syohin.jissai_genka, syohin.yotei_genka,
       syohin.kibou_kouri_kakaku kibou_kouri_kakaku,
       decode( syohin.seihin_syohin_kbn, 0, '', to_char( syohin.seihin_syohin_kbn ) || ':' || mei_seisyo.meisyo ) seihin_syohin_kbn,
       decode( trim( seisankoku_code ), '0', '', null, '', to_char( seisankoku_code ) || ':' || mei_seisankoku.meisyo ) seisankoku_code,
       decode( trim( syohin.siiresaki_code ), '0', '', null, '', to_char( syohin.siiresaki_code ) || ':' || trim( kojyo.kojyo_mei ) ) siiresaki_code_x,
       decode( syohin.siirehin_flg, 1, '1:対象', '' ) siirehin_flg,
       decode( syohin.syohin_kukaku_kbn, 0, '', to_char( syohin_kukaku_kbn ) || ':' || mei_kikaku.meisyo ) syohin_kukaku_kbn,
       decode( syohin.bettyu_running_flg, 1, '1:対象', '' ) bettyu_running_flg,
       decode( trim( syohin.tokui_code ), '0', '', null, '', to_char( syohin.tokui_code ) || ':' || trim( tokui.tokui_mei ) ) tokui_code_x,
       decode( trim( syohin.bukken_code ), '0', '', null, '', to_char( syohin.bukken_code ) || ':' || trim( bukken.bukken_mei ) ) bukken_code_x,
       decode( syohin.tokutei_tokui_kbn, 0, '', to_char( syohin.tokutei_tokui_kbn ) || ':' || mei_tokutei.meisyo ) tokutei_tokui_kbn,
       syohin.irisu, syohin.ball_irisu,
       decode( syohin.tani_kbn, 0, '', to_char( syohin.tani_kbn ) || ':' || mei_tani.meisyo ) tani_kbn,
       decode( syohin.syozai_kbn, 0, '', to_char( syohin.syozai_kbn ) || ':' || mei_syozai.meisyo ) syozai_kbn,
       decode( syohin.zaiko_kanri_flg, 1, '1:対象', '' ) zaiko_kanri_flg,
       decode( syohin.suryo_keisan_flg, 1, '1:対象', '' ) suryo_keisan_flg,
       decode( syohin.kingaku_keisan_flg, 1, '1:対象', '' ) kingaku_keisan_flg,
       decode( syohin.arari_keisan_flg, 1, '1:対象', '' ) arari_keisan_flg,
       decode( syohin.kankyo_hairyo_flg, 1, '1:対象', '' ) kankyo_hairyo_flg,
       decode( syohin.kuuyu_kinsi_flg, 1, '1:対象', '' ) kuuyu_kinsi_flg,
       decode( syohin.syokuti_flg, 1, '1:対象', '' ) syokuti_flg,
       decode( syohin.bhin_flg, 1, '1:対象', '' ) bhin_flg,
       decode( trim( syohin.daibunrui_code ), '0', '', null, '', to_char( syohin.daibunrui_code ) || ':' || trim( bunrui.daibunrui_mei ) ) daibunrui_code_x,
       decode( trim( syohin.tyubunrui_code ), '0', '', null, '', to_char( syohin.tyubunrui_code ) || ':' || trim( bunrui.tyubunrui_mei ) ) tyubunrui_code_x,
       decode( trim( syohin.syobunrui_code ), '0', '', null, '', to_char( syohin.syobunrui_code ) || ':' || trim( bunrui.syobunrui_mei ) ) syobunrui_code_x,
       decode( trim( syohin.group_code ), '0', '', null, '', to_char( syohin.group_code ) || ':' || trim( groupm.group_mei ) ) group_code_x,
       decode( trim( syohin.brand_code ), '0', '', null, '', to_char( syohin.brand_code ) || ':' || trim( brand.brand_mei ) ) brand_code_x,
       decode( trim( syohin.subbrand_code ), '0', '', null, '', to_char( syohin.subbrand_code ) || ':' || trim( subbrand.brand_mei ) ) subbrand_code_x,
       decode( trim( syohin.season_code ), '0', '', null, '', to_char( syohin.season_code ) || ':' || trim( mei_season.meisyo ) ) season_code_x,
       syohin.seriese_code,
       syohin.sozai,
       syohin.konritu_1, syohin.konritu_2, syohin.konritu_3, syohin.konritu_4, syohin.konritu_5,
       syohin.konritu_6, syohin.konritu_7, syohin.konritu_8, syohin.konritu_9, syohin.konritu_10,
       syohin.sunpo_tate, syohin.sunpo_yoko, syohin.sunpo_takasa, syohin.jyuryo, syohin.tijimi_ritu,
       syohin.yoki_kami, syohin.yoki_pla, syohin.housou_kami, syohin.housou_pla,
       syohin.touroku_date,
       musert.user_mei touroku_user_mei,
       syohin.henkou_date,
       muserh.user_mei henkou_user_mei
  from m_syohin syohin,
       ( select to_number( meisyo_code ) meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
          where meisyo_kbn	= 160
       ) mei_bumon,
       ( select to_number( meisyo_code ) meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
           where meisyo_kbn	= 122
       ) mei_haiban,
       ( select to_number( meisyo_code ) meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
           where meisyo_kbn	= 123
       ) mei_hairiyu,
       ( select to_number( meisyo_code ) meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
           where meisyo_kbn	= 131
       ) mei_kakaku_grp,
       ( select to_number( meisyo_code ) meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
           where meisyo_kbn	= 159
       ) mei_tankahyo,
       ( select to_number( meisyo_code ) meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
           where meisyo_kbn	= 126
       ) mei_seisyo,
       ( select meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
           where meisyo_kbn	= 130
       ) mei_seisankoku,
       ( select to_number( meisyo_code ) meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
           where meisyo_kbn	= 120
       ) mei_kikaku,
       ( select to_number( meisyo_code ) meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
           where meisyo_kbn	= 129
       ) mei_tokutei,
       ( select to_number( meisyo_code ) meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
           where meisyo_kbn	= 125
       ) mei_tani,
       ( select to_number( meisyo_code ) meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
           where meisyo_kbn	= 127
       ) mei_syozai,
       ( select to_number( meisyo_code ) meisyo_code,
                meisyo_1 meisyo
           from m_meisyo
           where meisyo_kbn	= 121
       ) mei_season,
       ( select kojyo_code, kojyo_mei
           from sy_m_kojyo
          where bumon_code	= 9
       ) kojyo,
       m_tokui tokui,
       m_bukken bukken,
       m_bunrui bunrui,
       m_group groupm,
       m_brand brand,
       m_brand subbrand,
       m_gtin gtin,
       m_user musert,
       m_user muserh
 where syohin.bumon_code			= mei_bumon.meisyo_code(+)
   and syohin.haiban_kbn			= mei_haiban.meisyo_code(+)
   and syohin.haiban_riyu_kbn		= mei_hairiyu.meisyo_code(+)
   and syohin.kakaku_group_kbn		= mei_kakaku_grp.meisyo_code(+)
   and syohin.tankahyo_hakkou_kbn	= mei_tankahyo.meisyo_code(+)
   and syohin.seihin_syohin_kbn		= mei_seisyo.meisyo_code(+)
   and syohin.seisankoku_code		= mei_seisankoku.meisyo_code(+)
   and syohin.syohin_kukaku_kbn		= mei_kikaku.meisyo_code(+)
   and syohin.tokutei_tokui_kbn		= mei_tokutei.meisyo_code(+)
   and syohin.tani_kbn				= mei_tani.meisyo_code(+)
   and syohin.syozai_kbn			= mei_syozai.meisyo_code(+)
   and syohin.season_code			= mei_season.meisyo_code(+)
   and syohin.siiresaki_code		= kojyo.kojyo_code(+)
   and syohin.tokui_code			= tokui.tokui_code(+)
   and syohin.bukken_code			= bukken.bukken_code(+)
   and syohin.daibunrui_code		= bunrui.daibunrui_code(+)
   and syohin.tyubunrui_code		= bunrui.tyubunrui_code(+)
   and syohin.syobunrui_code		= bunrui.syobunrui_code(+)
   and syohin.group_code			= groupm.group_code(+)
   and syohin.brand_code			= brand.brand_code(+)
   and syohin.subbrand_code			= subbrand.brand_code(+)
   and syohin.sku_code				= gtin.sku_code(+)
   and gtin.code_kbn				= 1
   and syohin.touroku_user_id		= musert.user_id(+)
