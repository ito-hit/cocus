                min( seisankoku_code ) seisankoku_code,
                min( yoki_kami ) yoki_kami,
                min( yoki_pla ) yoki_pla,
                min( housou_kami ) housou_kami,
                min( housou_pla ) housou_pla,
                min( sunpo_tate ) sunpo_tate,
                min( sunpo_yoko ) sunpo_yoko,
