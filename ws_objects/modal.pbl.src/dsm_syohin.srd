                         syo.konritu_1,syo.konritu_2,syo.konritu_3,syo.konritu_4,syo.konritu_5,
                         syo.konritu_6,syo.konritu_7,syo.konritu_8,syo.konritu_9,syo.konritu_10,
                         syo.seisankoku_code,
                         syo.yoki_kami,syo.yoki_pla,
                         syo.housou_kami,syo.housou_pla,
                         syo.sunpo_tate,syo.sunpo_yoko,syo.sunpo_takasa,
                         syo.jyuryo,syo.tijimi_ritu,
                         syo.flg_1,syo.flg_2,syo.flg_3,syo.flg_4,syo.flg_5,
                         syo.kbn_1,syo.kbn_2,syo.kbn_3,syo.kbn_4,syo.kbn_5,
                         syo.touroku_kbn,
                         syo.touroku_date,syo.touroku_user_id,syo.touroku_client_id,syo.touroku_pg_id,
                         syo.henkou_date,syo.henkou_user_id,syo.henkou_client_id,syo.henkou_pg_id,
                         syo.kousin_date,syo.kousin_user_id,syo.kousin_client_id,syo.kousin_pg_id,
                         syo.haita_flg
                    from m_syohin syo,
                         ( select gtin_code, sku_code, code_kbn
                             from m_gtin
                            where code_kbn	= 1
                         ) gti
                   where syo.sku_code	= gti.sku_code(+)
                     and syo.hin_ban	like '%' || trim( :p_hin_ban ) || '%'
                     and syo.hin_mei	like '%' || trim( :p_hin_mei ) || '%'
                     and gti.gtin_code	like '%' || trim( :p_gtin_code ) || '%'
                )
          group by syohin_code, iro_no, size_code
       ) grp,
       ( select to_number( meisyo_code ) haiban_code,
                decode( meisyo_2, 'ー', '', meisyo_2 ) haiban_mei
           from m_meisyo
          where meisyo_kbn	= 122
       ) meh
 where grp.haiban_kbn	= meh.haiban_code(+)
 order by syohin_code, iro_no" arguments=(("p_hin_ban", string),("p_hin_mei", string),("p_gtin_code", string))  sort="syohin_code A iro_no A " )
